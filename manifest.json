{
  "manifest_version": 2,
  "name": "gbf-window-controll",
  "version":"0.0.1",
  "description":"お空の窓をジャンプさせる",
  "browser_action": {
    "default_popup": "dest/html/popup.html"
  },
  "background":{
    "scripts":[
      "dest/js/background.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "dest/html/oauthPage.html*",
    "dest/html/retweetSet.html"
  ]
}
